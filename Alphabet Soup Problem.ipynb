   ],
   "source": [
    "def alphabet_soup(w):\n",
    "    sorted_chars = sorted(w, key=str.lower)\n",
    "    joined_chars = \"\".join(sorted_chars)\n",
    "    return joined_chars\n",
    "    \n",
    "\n",
    "w = input(\"Enter your word: \")\n",
    "print(f'Your word in alphabetical order: {alphabet_soup(w)}')"
   ]
  }
